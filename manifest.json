{
    "manifest_version": 2,
    "name": "Sudoku Solver",
    "description": "Extention that solves sudokus.",
    "version": "1.0.0",
    "icons": {
        "128": "icon_128.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "sudokugame.js",
                "sudokusolver.js",
                "content.js"
            ],
            "run_at": "document_end"
        }
    ],
    "background": {
        "scripts": []
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}